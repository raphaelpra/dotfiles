{
    "cond": [
        "",
        "if () {",
        "{body}",
        "}",
        ""
    ],
    "function": [
        "",
        "function {name}() {",
        "{body}",
        "}",
        ""
    ],
    "functionCall": [
        "{name}();",
        ""
    ],
    "iife": [
        "",
        "(function () {",
        "{body}",
        "})();",
        ""
    ],

    "arrowFunction": [
        "() => {",
        "{body}",
        "}"
    ],

    "generator": [
        "function* {name} () {",
        "{body}",
        "}"
    ],

    "asyncFunction": [
        "",
        "async function {name}() {",
        "{body}",
        "}",
        ""
    ],

    "newVariable": [
        "{varType} {name} = {value};",
        ""
    ],

    "tryCatch": [
        "try {",
        "{body}",
        "} catch (e) {",
        "console.log(e.message);",
        "}"
    ],

    "addExport": {
        "single": [
            "",
            "module.exports.{functionName} = {functionName}"
        ],
        "newExport": [
            "",
            "module.exports = {",
            "\t{functionName}: {functionName}",
            "}"
        ],
        "objectAddition": [
            "",
            "\t{functionName}: {functionName},"
        ]
    }
}